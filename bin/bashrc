c() {
  if [ "$1" == "w" ]; then
    cd *.wiki
  elif [ -e ~/github ]; then
    cd ~/github
  else
    cd ~
  fi
  if [ "$1" == "s" ]; then
    cd FireSight
  elif [ "$1" == "r" ]; then
    cd FireREST
  elif [ "$1" == "mote" ] || [ "$1" == "m" ]; then
    cd FireMote
  elif [ "$1" == "f" ]; then
    cd FireFUSE
  elif [ "$1" == "pixel" ]; then
    cd ~/pixel
  fi
}

alias sa='eval `ssh-agent -s`; ssh-add ~/.ssh/id_rsa'
